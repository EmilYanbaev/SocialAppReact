wrapper\">\r\n                <LeftSiteBar />\r\n                <ContentPage />\r\n                <RightSiteBar />\r\n            </div>\r\n        </div>\r\n    )\r\n}\r\n\r\n\r\n\r\n\r\nconst mapStateToProps = (state) => {\r\n    return {\r\n        initialized: state.general.initialized,\r\n    }\r\n}\r\nexport default compose(withErrorBoundary,\r\n    connect(mapStateToProps, { initialize: initialize }))\r\n    (GeneralContainer)","C:\\Project Social\\SocialV-2\\SocialAppReact\\social-app-2.0\\src\\utils\\validators\\validators.js",["173","174"],"export const required = (value)=>{\r\n   return  value ? undefined: \"Require Form\"\r\n}\r\n\r\nexport const maxLengthCreator = (length)=>{\r\n   return (value)=>{\r\n      return value.length>length ?`Max length ${length}`:undefined;\r\n   }\r\n}\r\n\r\nexport const isValidURL = (value) =>{\r\n   if(!value)\r\n   return undefined;\r\n   \r\n   let res = value?.match(/(http(s)?:\\/\\/.)?(www\\.)?[-a-zA-Z0-9@:%._\\+~#=]{2,256}\\.[a-z]{2,6}\\b([-a-zA-Z0-9@:%_\\+.~#?&//=]*)/g);\r\n   return (res !== null)? undefined:\"invalid Url\"\r\n };","C:\\Project Social\\SocialV-2\\SocialAppReact\\social-app-2.0\\src\\component\\mainComponent\\LoginPage\\Page\\LoginForm.jsx",["175"],"import { Field, reduxForm } from \"redux-form\";\r\nimport style from \"./Login.module.css\"\r\nimport { maxLengthCreator, required } from '../../../../utils/validators/validators';\r\nimport { Button } from '../../../otherComponent/GroupComponent';\r\n\r\nconst maxLength = maxLengthCreator(30);\r\n\r\nlet LoginForm = ({ handleSubmit, captchaUrl, error }) => {\r\n    return (\r\n        <form className={style.form_group} onSubmit={handleSubmit}>\r\n\r\n            {CreateField(\"inputEmail\", \"email\", Input, \"text\", [required, maxLength], \"Email enter\", \"Email address\",)}\r\n            {CreateField(\"inputPass\", \"password\", Input, \"password\", [required], \"Password\", \"Password\",)}\r\n\r\n            { error && <p className={style.error} >{error}</p>}\r\n            {captchaUrl &&\r\n                <div className={style.captchaControl}>\r\n                    <img src={captchaUrl} />\r\n                    {CreateField(\"captcha\", \"captcha\", Input, \"text\", [required], \"\", \"Captcha\",)}\r\n                </div>\r\n            }\r\n\r\n            <div className={style.control}>\r\n                <div className={style.control__checkBox}>\r\n                    {CreateField(\"checkbox\", \"rememberMe\", \"input\", \"checkbox\", [], \"\", \"Remember Me\", { className: style.checkBox })}\r\n                </div>\r\n\r\n                <Button type=\"submit\">Sign In</Button>\r\n            </div>\r\n        </form>\r\n    )\r\n}\r\n\r\n\r\nexport default reduxForm({ form: \"login\" })(LoginForm)\r\n\r\nconst CreateField = (id, name, component, type, validators, placeholder, text, props) => {\r\n    return (\r\n        <>\r\n            <label htmlFor={id} className={style.label}>{text}</label>\r\n            <Field id={id} name={name} component={component} type={type} validate={validators} placeholder={placeholder} {...props} />\r\n        </>\r\n    )\r\n}\r\n\r\nconst Input = ({ input, meta, ...props }) => {\r\n    const hasError = meta.touched && meta.error\r\n    return (\r\n        <div className={style.inputWrapp}>\r\n            <input className={style.inputWrapp__input + \" \" + (hasError ? style.warning : \"\")} {...input} {...props} />\r\n            {hasError && <p className={style.error}>{meta.error}</p>}\r\n        </div>\r\n    )\r\n}\r\n\r\n\r\n","C:\\Project Social\\SocialV-2\\SocialAppReact\\social-app-2.0\\src\\component\\baseComponent\\Header\\HeaderContainer.jsx",[],"C:\\Project Social\\SocialV-2\\SocialAppReact\\social-app-2.0\\src\\hoc\\withErrorBoundary.js",[],"C:\\Project Social\\SocialV-2\\SocialAppReact\\social-app-2.0\\src\\hoc\\withHiddenSitebar.js",[],"C:\\Project Social\\SocialV-2\\SocialAppReact\\social-app-2.0\\src\\utils\\other.js",[],"C:\\Project Social\\S